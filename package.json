		"markdown-to-jsx": "^6.9.3",
		"next": "^8.0.3",
		"next-page-transitions": "^1.0.0-beta.1",
		"react": "^16.8.3",
		"react-animations": "^1.0.0",
		"react-anime": "^2.2.0",
		"react-div-100vh": "^0.3.2",
		"react-dom": "^16.8.3",
		"react-image-lightbox": "^5.1.0",
		"react-images": "^0.5.19",
		"react-markdown": "^4.0.6",
		"react-modal": "^3.8.1",
		"react-netlify-form": "^2.1.5",
		"react-player": "^1.10.0",
		"react-reveal": "^1.2.2",
		"react-sticky": "^6.0.3",
		"styled-components": "^4.1.3"
	},
