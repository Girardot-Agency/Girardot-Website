{
	"name": "girardot-website",
	"version": "0.0.5-alpha",
	"description": "",
	"main": "index.js",
	"scripts": {
		"========== Clean": "==========",
		"clean": "rimraf _out/** exports/uploads/**",
		"clean:cache": "rimraf .cache/uploads",
		"========== Prebuild": "==========",
		"prebuild": "npm run clean && npm run prebuild:media && npm run prebuild:exportmap",
		"prebuild:media": "node ./lib/build-media.js",
		"prebuild:exportmap": "node ./lib/build-export-map.js",
		"========== NextJS": "==========",
		"next:build": "next build",
		"next:dev": "next",
		"next:export": "npm run next:build && next export --outdir _out",
		"========== Development": "==========",
		"dev": "NODE_ENV=development npm run next:dev",
		"dev:clean": "NODE_ENV=development npm run prebuild && npm run next:dev",
		"========== Deploy": "==========",
		"deploy": "npm run prebuild && npm run next:build && npm run next:export",
		"deploy:dev": "NODE_ENV=development npm run deploy",
		"deploy:public": "NODE_ENV=production DEPLOY_BRANCH=public npm run deploy",
		"deploy:preview": "NODE_ENV=production DEPLOY_BRANCH=preview npm run deploy"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
